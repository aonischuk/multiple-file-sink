{
  "metadata": {
    "spec-version": "1.3"
  },
  "configuration-groups": [
    {
      "label": "Multiple Snapshot Fileset Sink",
      "properties": [
        {
          "widget-type": "select",
          "label": "Compression Codec",
          "name": "compressionCodec",
          "widget-attributes": {
            "values": [
              "None",
              "Snappy",
              "Deflate"
            ],
            "default": "None"
          }
        },
        {
          "widget-type":"json-editor",
          "name":"fileProperties"
        }
      ]
    }
  ],
  "outputs": [
    {
      "name": "schema",
      "widget-type": "schema",
      "widget-attributes": {
        "schema-types": [
          "boolean",
          "int",
          "long",
          "float",
          "double",
          "string"
        ],
        "schema-default-type": "string"
      }
    }
  ],
  "jump-config": {
    "datasets": [{
      "ref-property-name": "name"
    }]
  }
}
